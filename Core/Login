--Variables--

local notLogin = true
local w,h = term.getSize()
term.redirect(term.native())

--Code--

--Startup script--
term.setBackgroundColor(colors.white)
term.clear()

--Main Loop--

while notLogin do
	GUI.ResetButtons()
	GUI.cPrint("Login",colors.black,nil,3,3)
	GUI.cPrint("Username: >",colors.gray,nil,3,6)
	GUI.cPrint("Password: >",colors.gray,nil,3,8)
	os.pullEvent()
end