--[[
	System API
	for 
	TheOS
	by Creator
]]--

function reboot()
	os.reboot()
end

function newTask(name,title,...)
	local mainPath = "TheOS/Programs/"..name..".app/Main.lua"
	print(mainPath)
	if fs.exists(mainPath) then
		print("yeah")
		local func = loadfile(mainPath)
		newRoutine(name,title,func,...)
		os.pullEvent()
	end
	return false
end